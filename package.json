{
  "name": "minimalist-teleprompter",
  "version": "0.0.0",
  "description": "This project is going to explore how to use Cursor, v0, Next.js, and Vercel to build and deploy a simple web app.",
  "scripts": {
    "dev": "npm run dev --prefix apps/teleprompter",
    "build": "npm run build --prefix apps/teleprompter",
    "start": "npm run start --prefix apps/teleprompter",
    "lint": "npm run lint --prefix apps/teleprompter",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up",
    "docker:up:detach": "docker compose up -d",
    "docker:down": "docker compose down"
  },
